{
  "name": "james-albert-cosmetics",
  "version": "1.0.0",
  "description": "james-albert-server",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "test": "npm run lint && NODE_ENV=test mocha -u tdd --reporter spec --exit"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "mocha": "^5.2.0",
    "prettier": "^1.13.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "cookie-parser": "^1.4.4",
    "date-fns": "^1.30.1",
    "dotenv": "^6.0.0",
    "eslint": "^5.16.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "firebase": "^5.9.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.0",
    "mysql2": "^1.6.5",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-router-dom": "^5.0.0",
    "sequelize": "^5.7.1",
    "sequelize-cli": "^5.4.0",
    "typeface-roboto": "0.0.54"
  }
}
